{
  "name": "ff3300/k5-base",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "getkirby/cms": "^5.1",
    "sylvainjule/locator": "^2.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "kirby-cms-path": "kirby",
    "kirby-plugin-path": "site/plugins"
  },
  "prefer-stable": true,
  "minimum-stability": "stable"
}
